{
  "name": "dev-life-tycoon",
  "version": "0.1.0",
  "private": true,
  "description": "Dev Life Tycoon MVP - Electron + React + SQLite",
  "main": "dist/main/main/main.js",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:renderer\" \"npm:dev:main\"",
    "dev:renderer": "vite --port 5173 --strictPort",
    "dev:main": "concurrently -k \"npm:dev:main:build\" \"npm:dev:main:run\"",
    "dev:main:build": "tsc -p tsconfig.main.json --watch --preserveWatchOutput",
    "dev:main:run": "wait-on tcp:5173 dist/main/main/main.js && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "vite build",
    "build:main": "tsc -p tsconfig.main.json",
    "test": "vitest run"
  },
  "dependencies": {
    "electron": "~35.7.5",
    "react": "~19.2.4",
    "react-dom": "~19.2.4"
  },
  "devDependencies": {
    "@types/node": "~22.19.10",
    "@types/react": "~19.2.13",
    "@types/react-dom": "~19.2.3",
    "@vitejs/plugin-react": "~4.7.0",
    "concurrently": "~9.2.1",
    "cross-env": "~7.0.3",
    "typescript": "~5.9.3",
    "vite": "~6.4.1",
    "vitest": "~3.2.4",
    "wait-on": "~8.0.5"
  }
}